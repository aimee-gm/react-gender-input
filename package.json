	"license": "MIT",
	"bugs": {
		"url": "https://github.com/aimee-gm/react-gender-input/issues"
	},
	"homepage": "https://github.com/aimee-gm/react-gender-input#readme",
	"devDependencies": {
		"@types/code": "^4.0.4",
		"@types/enzyme-adapter-react-16": "^1.0.3",
		"@types/mocha": "^5.2.5",
		"@types/react": "^16.7.4",
		"@types/react-dom": "^16.0.9",
		"@types/react-syntax-highlighter": "0.0.8",
		"@types/sinon": "^5.0.5",
