	"description": "A simple react gender component for better gender data collection",
	"main": "dist/index.js",
	"scripts": {
		"depcheck": "npm-check --skip-unused",
		"build": "tsc",
		"test": "mocha test/gender-input.spec.tsx",
		"coverage": "nyc mocha test/*.spec.tsx",
		"demo:dev": "parcel -p 8080 watch demo/index.html",
