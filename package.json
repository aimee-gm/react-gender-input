{
	"name": "react-gender-input",
	"version": "0.1.0",
	"description": "A simple react gender component for better gender data collection",
	"main": "index.js",
	"scripts": {
		"build": "tsc",
		"test:demo": "mocha --require ./test/demo/.setup.ts test/demo/*.spec.tsx",
		"test": "mocha test/*.spec.tsx",
		"test:all": "npm test && npm run test:demo",
		"coverage": "nyc npm run test:all",
		"coveralls": "nyc report --reporter=text-lcov | coveralls",
		"demo:dev": "parcel -p 8080 watch demo/index.html",
		"demo:build": "parcel build demo/index.html --public-url /react-gender-input/ --out-dir dist/demo"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ajgamble-milner/react-gender-input.git"
	},
	"keywords": [
		"react",
		"gender",
		"form",
		"input"
	],
	"author": "Aimee Gamble-Milner (https://github.com/ajgamble-milner)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ajgamble-milner/react-gender-input/issues"
	},
	"homepage": "https://github.com/ajgamble-milner/react-gender-input#readme",
	"devDependencies": {
		"@types/chai": "^4.1.4",
		"@types/enzyme-adapter-react-16": "^1.0.3",
		"@types/mocha": "^5.2.5",
		"@types/react": "^16.4.18",
		"@types/react-dom": "^16.0.9",
		"@types/react-syntax-highlighter": "0.0.7",
		"@types/sinon": "^5.0.5",
		"@types/webpack": "^4.4.17",
		"autobind-decorator": "^2.1.0",
		"chai": "^4.1.2",
		"coveralls": "^3.0.2",
		"enzyme": "^3.6.0",
		"enzyme-adapter-react-16": "^1.5.0",
		"husky": "^1.1.2",
		"jsdom": "^12.2.0",
		"jsdom-global": "^3.0.2",
		"mocha": "^5.2.0",
		"nyc": "^13.1.0",
		"parcel-bundler": "^1.10.3",
		"prettier": "1.14.3",
		"pretty-quick": "^1.8.0",
		"react": "^16.5.1",
		"react-dom": "^16.5.1",
		"react-syntax-highlighter": "^9.0.0",
		"sass": "^1.14.3",
		"sinon": "^7.0.0",
		"source-map-support": "^0.5.9",
		"source-sans-pro": "^2.20.2",
		"ts-node": "^7.0.1",
		"typescript": "^3.1.3"
	},
	"peerDependencies": {
		"react": "^16.5.1",
		"react-dom": "^16.5.1"
	}
}
